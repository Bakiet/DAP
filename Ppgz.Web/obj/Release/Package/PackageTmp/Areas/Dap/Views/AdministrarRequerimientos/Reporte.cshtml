                        <h4 style="color:darkblue;">@Html.DisplayNameFor(o => o.StatusRequerimientos)</h4>
                    </div>
                    <div class="col-4 col-sm-6" style="">
                        @Html.DisplayFor(o => o.StatusRequerimientos)
                    </div>
                </div>

