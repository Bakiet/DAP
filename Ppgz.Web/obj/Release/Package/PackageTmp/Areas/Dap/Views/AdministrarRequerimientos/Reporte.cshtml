                    <div class="col-8 col-sm-6">

                        <h4 style="color:darkblue;">@Html.DisplayNameFor(o => o.AccionesEjecutadas)</h4>
                    </div>
                    <div class="col-4 col-sm-6" style="">
                        @Html.DisplayFor(o => o.AccionesEjecutadas)
